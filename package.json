{
  "name": "clonegallery",
  "version": "1.0.0",
  "description": "AI-Enhanced Responsive Image Gallery Platform",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:ci": "start-server-and-test start http://localhost cypress:run",
    "test:coverage": "nyc cypress run",
    "cypress:run": "cypress run --browser chrome --headless",
    "cypress:open": "cypress open",
    "lint": "eslint resources/js --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint resources/js --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write resources/js/**/*.{js,jsx,ts,tsx,css,scss}",
    "lighthouse:desktop": "lighthouse http://localhost --preset=desktop --output=json --output-path=reports/lighthouse-desktop.json",
    "lighthouse:mobile": "lighthouse http://localhost --preset=mobile --output=json --output-path=reports/lighthouse-mobile.json",
    "lighthouse:ci": "lhci autorun"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.0",
    "axe-core": "^4.8.0",
    "cypress": "^13.6.0",
    "cypress-axe": "^1.5.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.11.0",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.13.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "start-server-and-test": "^2.0.3",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "alpinejs": "^3.13.0",
    "axios": "^1.6.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/support/step_definitions"
  },
  "nyc": {
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "cypress/**/*.*"
    ],
    "reporter": [
      "text",
      "json",
      "html"
    ],
    "all": true
  }
}
